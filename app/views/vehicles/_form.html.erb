<div class="container container-form">
  <%= simple_form_for(vehicle) do |f| %>
    <%= f.input :name %>
    <%= f.input :category, collection: Vehicle::CATEGORIES %>
    <%= f.input :city %>
    <%= f.input :price %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.submit %>
  <% end %>
</div>
